[Unit]
Description=EverestPortal
Wants=NetworkManager-wait-online.service network.target network-online.target dbus.service
After=NetworkManager-wait-online.service network-online.target


[Service]
Type=simple
WorkingDirectory=/opt/testportal_ts/source
OOMScoreAdjust=-1000
ExecStart=/opt/testportal_ts/source/webserver/gunicorn/run.sh
ExecStop=
TimeoutSec=300


[Install]
WantedBy=multi-user.target

